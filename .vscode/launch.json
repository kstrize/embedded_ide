    "version": "0.2.0",
    "configurations": [
        {
            "type": "gdb",
            "request": "attach",
            "name": "App remote GDB",
            "gdbpath": "/usr/bin/gdb-multiarch",
            "executable": "${config:APP_BIN_DIR}",
            "target": "${config:TARGET_IP}:${config:DEBUG_PORT}",
            "cwd": "${workspaceRoot}",
            "remote": true
        }
    ]
}